{
	"name": "svelte-budget-planner",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host 0.0.0.0 --port 3000",
		"dev:nvim": "node --inspect ./node_modules/.bin/vite dev --host 0.0.0.0 --port 3000",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"test:e2e": "playwright test",
		"coverage": "vitest run --coverage",
		"lint": "prettier --plugin=prettier-plugin-svelte --plugin=prettier-plugin-tailwindcss --plugin=prettier-plugin-organize-imports . --check . && eslint .",
		"format": "prettier --plugin=prettier-plugin-svelte --plugin=prettier-plugin-tailwindcss --plugin=prettier-plugin-organize-imports . --write .",
		"prepare": "husky"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.2",
		"@eslint/js": "^9.35.0",
		"@playwright/test": "^1.55.0",
		"@skeletonlabs/skeleton": "^3.2.0",
		"@skeletonlabs/skeleton-svelte": "^1.5.1",
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/adapter-vercel": "^5.10.2",
		"@sveltejs/kit": "^2.37.0",
		"@sveltejs/vite-plugin-svelte": "^6.1.4",
		"@tailwindcss/forms": "^0.5.10",
		"@types/node": "^24.3.1",
		"@vitest/coverage-v8": "^3.2.4",
		"dotenv": "^17.2.2",
		"dotenv-expand": "^12.0.3",
		"eslint": "^9.35.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.1",
		"globals": "^16.3.0",
		"husky": "^9.1.7",
		"prettier": "^3.6.2",
		"prettier-plugin-organize-imports": "^4.2.0",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "^5.38.7",
		"svelte-check": "^4.3.1",
		"tailwindcss": "^4.1.4",
		"tslib": "^2.8.1",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.42.0",
		"vercel": "^47.0.5",
		"vite": "^7.1.4",
		"vitest": "^3.0.2"
	},
	"type": "module",
	"dependencies": {
		"@auth/sveltekit": "^1.10.0",
		"@floating-ui/dom": "^1.7.4",
		"@neondatabase/serverless": "^0.4.26",
		"@tailwindcss/vite": "^4.1.13",
		"@vercel/otel": "^1.13.0",
		"kysely": "^0.27.0",
		"kysely-neon": "^1.3.0",
		"kysely-plugin-prefix": "^1.2.2",
		"lucide-svelte": "^0.542.0",
		"svelte-i18n": "^4.0.1"
	},
	"engines": {
		"node": "22.x"
	}
}

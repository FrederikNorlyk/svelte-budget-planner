{
	"name": "svelte-budget-planner",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host 0.0.0.0 --port 3000",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"coverage": "vitest run --coverage",
		"lint": "prettier --plugin=prettier-plugin-svelte --plugin=prettier-plugin-tailwindcss --plugin=prettier-plugin-organize-imports . --check . && eslint .",
		"format": "prettier --plugin=prettier-plugin-svelte --plugin=prettier-plugin-tailwindcss --plugin=prettier-plugin-organize-imports . --write ."
	},
	"devDependencies": {
		"@skeletonlabs/skeleton": "^2.10.0",
		"@skeletonlabs/tw-plugin": "^0.4.0",
		"@sveltejs/adapter-auto": "^3.2.1",
		"@sveltejs/adapter-vercel": "^5.3.0",
		"@sveltejs/kit": "^2.5.10",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/forms": "^0.5.7",
		"@types/node": "^20.14.2",
		"@typescript-eslint/eslint-plugin": "^7.10.0",
		"@typescript-eslint/parser": "^7.10.0",
		"@vitest/coverage-v8": "^1.6.0",
		"autoprefixer": "^10.4.19",
		"dotenv": "^16.4.5",
		"dotenv-expand": "^11.0.6",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.39.0",
		"postcss": "^8.4.38",
		"prettier": "^3.2.5",
		"prettier-plugin-organize-imports": "^3.2.4",
		"prettier-plugin-svelte": "^3.2.3",
		"prettier-plugin-tailwindcss": "^0.5.14",
		"svelte": "^4.2.17",
		"svelte-check": "^3.8.0",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.6.2",
		"typescript": "^5.4.5",
		"vercel": "^34.2.4",
		"vite": "^5.2.11",
		"vitest": "^1.6.0"
	},
	"type": "module",
	"dependencies": {
		"@auth/sveltekit": "^1.1.0",
		"@floating-ui/dom": "^1.6.5",
		"@vercel/postgres": "^0.8.0",
		"@vercel/postgres-kysely": "^0.8.0",
		"kysely": "^0.27.3",
		"kysely-plugin-prefix": "^1.1.1"
	}
}

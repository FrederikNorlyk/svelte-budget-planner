{
	"name": "svelte-budget-planner",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host 0.0.0.0 --port 3000",
		"dev:nvim": "node --inspect ./node_modules/.bin/vite dev --host 0.0.0.0 --port 3000",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"test:e2e": "playwright test",
		"coverage": "vitest run --coverage",
		"lint": "prettier --plugin=prettier-plugin-svelte --plugin=prettier-plugin-tailwindcss --plugin=prettier-plugin-organize-imports . --check . && eslint .",
		"format": "prettier --plugin=prettier-plugin-svelte --plugin=prettier-plugin-tailwindcss --plugin=prettier-plugin-organize-imports . --write .",
		"prepare": "husky"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.9",
		"@eslint/js": "^9.27.0",
		"@playwright/test": "^1.52.0",
		"@skeletonlabs/skeleton": "^3.1.3",
		"@skeletonlabs/skeleton-svelte": "^1.2.3",
		"@sveltejs/adapter-auto": "^6.0.1",
		"@sveltejs/adapter-vercel": "^5.7.2",
		"@sveltejs/kit": "^2.21.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/forms": "^0.5.10",
		"@types/node": "^22.15.21",
		"@vitest/coverage-v8": "^3.1.4",
		"dotenv": "^16.5.0",
		"dotenv-expand": "^12.0.2",
		"eslint": "^9.27.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.9.0",
		"globals": "^16.1.0",
		"husky": "^9.1.7",
		"prettier": "^3.5.3",
		"prettier-plugin-organize-imports": "^4.1.0",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.33.1",
		"svelte-check": "^4.2.1",
		"tailwindcss": "^4.1.4",
		"tslib": "^2.8.1",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.32.1",
		"vercel": "^42.1.1",
		"vite": "^6.3.5",
		"vitest": "^3.0.2"
	},
	"type": "module",
	"dependencies": {
		"@auth/sveltekit": "^1.9.2",
		"@floating-ui/dom": "^1.7.0",
		"@neondatabase/serverless": "^0.4.26",
		"@tailwindcss/vite": "^4.1.7",
		"kysely": "^0.27.0",
		"kysely-neon": "^1.3.0",
		"kysely-plugin-prefix": "^1.2.2",
		"lucide-svelte": "^0.511.0",
		"svelte-i18n": "^4.0.1"
	},
	"engines": {
		"node": "22.x"
	}
}

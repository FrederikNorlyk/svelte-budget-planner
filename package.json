{
	"name": "svelte-budget-planner",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host 0.0.0.0 --port 3000",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"coverage": "vitest run --coverage",
		"lint": "prettier --plugin=prettier-plugin-svelte --plugin=prettier-plugin-tailwindcss --plugin=prettier-plugin-organize-imports . --check . && eslint .",
		"format": "prettier --plugin=prettier-plugin-svelte --plugin=prettier-plugin-tailwindcss --plugin=prettier-plugin-organize-imports . --write .",
		"prepare": "husky"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.24.0",
		"@playwright/test": "^1.51.1",
		"@skeletonlabs/skeleton": "^3.1.2",
		"@skeletonlabs/tw-plugin": "^0.4.1",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-vercel": "^5.7.0",
		"@sveltejs/kit": "^2.20.4",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/forms": "^0.5.10",
		"@types/node": "^22.14.0",
		"@typescript-eslint/eslint-plugin": "^8.29.0",
		"@typescript-eslint/parser": "^8.29.0",
		"@vitest/coverage-v8": "^3.1.1",
		"autoprefixer": "^10.4.21",
		"dotenv": "^16.4.7",
		"dotenv-expand": "^12.0.1",
		"eslint": "^9.24.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-svelte": "^3.5.1",
		"globals": "^16.0.0",
		"husky": "^9.1.7",
		"postcss": "^8.5.3",
		"prettier": "^3.5.3",
		"prettier-plugin-organize-imports": "^4.1.0",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.25.7",
		"svelte-check": "^4.1.5",
		"tailwindcss": "^3.4.17",
		"tslib": "^2.8.1",
		"typescript": "^5.8.3",
		"vercel": "^41.4.1",
		"vite": "^6.2.5",
		"vitest": "^3.0.2"
	},

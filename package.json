{
	"name": "svelte-budget-planner",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host 0.0.0.0 --port 3000",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"coverage": "vitest run --coverage",
		"lint": "prettier --plugin=prettier-plugin-svelte --plugin=prettier-plugin-tailwindcss --plugin=prettier-plugin-organize-imports . --check . && eslint .",
		"format": "prettier --plugin=prettier-plugin-svelte --plugin=prettier-plugin-tailwindcss --plugin=prettier-plugin-organize-imports . --write .",
		"prepare": "husky"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.1.0",
		"@eslint/js": "^9.14.0",
		"@playwright/test": "^1.48.2",
		"@skeletonlabs/skeleton": "^2.10.3",
		"@skeletonlabs/tw-plugin": "^0.4.0",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/adapter-vercel": "^5.4.7",
		"@sveltejs/kit": "^2.8.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@types/node": "^22.9.0",
		"@typescript-eslint/eslint-plugin": "^8.13.0",
		"@typescript-eslint/parser": "^8.13.0",
		"@vitest/coverage-v8": "^2.1.4",
		"autoprefixer": "^10.4.20",
		"dotenv": "^16.4.5",
		"dotenv-expand": "^11.0.6",
		"eslint": "^9.14.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.0",
		"globals": "^15.12.0",
		"husky": "^9.1.6",
		"postcss": "^8.4.47",
		"prettier": "^3.3.3",
		"prettier-plugin-organize-imports": "^4.1.0",
		"prettier-plugin-svelte": "^3.2.7",
		"prettier-plugin-tailwindcss": "^0.6.8",
		"svelte": "^5.1.13",
		"svelte-check": "^4.0.6",
		"tailwindcss": "^3.4.14",
		"tslib": "^2.8.1",
		"typescript": "^5.6.3",
		"vercel": "^37.14.0",
		"vite": "^5.4.10",
		"vitest": "^2.0.0"
	},
	"type": "module",
	"dependencies": {
		"@auth/sveltekit": "^1.7.3",
		"@floating-ui/dom": "^1.6.12",
		"@vercel/postgres": "^0.10.0",
		"@vercel/postgres-kysely": "^0.10.0",
		"kysely": "^0.27.4",
		"kysely-plugin-prefix": "^1.2.1",
		"svelte-i18n": "^4.0.1"
	},
	"engines": {
		"node": "20.x"
	}
}

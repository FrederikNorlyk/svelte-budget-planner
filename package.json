		"@vercel/postgres-kysely": "^0.10.0",
		"kysely": "^0.27.6",
		"kysely-plugin-prefix": "^1.2.2",
		"lucide-svelte": "^0.487.0",
		"svelte-i18n": "^4.0.1"
	},
	"engines": {
		"node": "22.x"
	}
